{
  "name": "@peterek/vite-plugin-font",
  "version": "1.2.2",
  "description": "Vite plugin for simple use of custom fonts",
  "keywords": [
    "vite",
    "plugin",
    "font",
    "fonts"
  ],
  "author": {
    "name": "Jiri Peterek",
    "email": "jiri@peterek.net"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peterekjs/vite-plugin-font/issues"
  },
  "homepage": "https://github.com/peterekjs/vite-plugin-font#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterekjs/vite-plugin-font.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/font.cjs",
      "default": "./dist/font.js"
    },
    "./src/*": "./src/*"
  },
  "typesVersions": {
    ">=5": {
      "index": [
        "./dist/index.d.ts"
      ]
    }
  },
  "types": "./dist/index.d.ts",
  "typescript": {
    "definition": "./dist/index.d.ts"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:dev": "vitest",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "glob": "^10.3.10"
  },
  "peerDependencies": {
    "vite": "^5.0.12"
  },
  "devDependencies": {
    "@types/node": "^20.11.15",
    "@vitest/coverage-v8": "^1.2.2",
    "typescript": "^5.3.3",
    "vite": "5.0.12",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.2"
  }
}
